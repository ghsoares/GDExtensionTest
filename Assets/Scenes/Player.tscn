[sub_resource type="Shader" id=11]
code = "shader_type canvas_item;

uniform vec2 velocity = vec2(0, 1f);
uniform float range = .5f;
uniform float normalMultiplier = 2f;
uniform sampler2D gradient;
uniform sampler2D noise;
uniform vec2 noiseMotion = vec2(0, 1f);
uniform float noiseMagnitude = .1f;
uniform vec2 velocityRange = vec2(15f, 128f);

void fragment() {
	vec2 uv = UV;
	
	COLOR = texture(TEXTURE, uv);
	
	vec2 n = texture(NORMAL_TEXTURE, uv).rg * 2f - 1f;
	n *= normalMultiplier;
	n.y = -n.y;
	n = normalize(n) * min(length(n), 1f);
	
	float d = dot(n, normalize(velocity)) * 2f;
	float l = (length(velocity) - velocityRange.x) / (velocityRange.y - velocityRange.x);
	d *= clamp(l, 0, 1);
	
	float ns = pow(texture(noise, uv + TIME * noiseMotion).r, 4f) * noiseMagnitude;
	
	d -= ns;
	
	d = clamp(d, 0, 1);
	
	vec4 c = texture(gradient, vec2(d, 0));
	
	COLOR.rgb = mix(COLOR.rgb, c.rgb, c.a);
}


"

