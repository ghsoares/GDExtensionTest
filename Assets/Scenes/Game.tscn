[sub_resource type="Shader" id=2]
code = "shader_type canvas_item;

uniform float ambientLight = 1f;
uniform sampler2D lightSources;
uniform int numLightSources;
uniform vec2 size;
uniform float radiusMultiply = 2f;
uniform sampler2D ditheringMatrix;
uniform vec2 ditheringTiling = vec2(32f, 32f);
uniform float ditheringThreshold = .5f;

float smin( float a, float b, float k ) {
    float h = clamp( 0.5+0.5*(b-a)/k, 0., 1. );
    return mix( b, a, h ) - k*h*(1.0-h);
}

float distSqr(vec2 a, vec2 b) {
	vec2 c = (b - a);
	float d = pow(c.x, 2f) + pow(c.y, 2f);
	return d;
}

float get_light(vec2 pos) {
	float finalLightIntensity = ambientLight;
	
	for (int i = 0; i < numLightSources; i++) {
		float x = float(i) / float(numLightSources) + .0001f;
		vec4 light = texture(lightSources, vec2(x, 0f));
		vec2 lightPos = light.rg * size;
		float lightRadius = light.b * radiusMultiply;
		float intensity = light.a;
		
		float dist = distance(lightPos, pos);
		
		float t = 1f - dist / lightRadius;
		t *= intensity;
		t = clamp(t, 0, 1);
		
		finalLightIntensity += t;
	}
	
	return finalLightIntensity;
}

float get_dithering(vec2 pos) {
	float d = texture(ditheringMatrix, pos / ditheringTiling).r;
	return d;
}

void fragment() {
	vec2 pos = UV * size;
	pos = floor(pos);
	float light = get_dithering(pos) - 1f + get_light(pos) * 2f;
	light = clamp(light, 0, 1);
	light = step(ditheringThreshold, light);
	
	vec4 col = texture(TEXTURE, UV);
	col.rgb *= light;
	COLOR = col;
}


"

