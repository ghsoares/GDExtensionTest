[sub_resource type="Shader" id=2]
code = "shader_type canvas_item;

uniform vec2 speed;
uniform float maxSafeSpeed = 16.0;
uniform float maxSpeed = 32.0;
uniform sampler2D noise;
uniform vec2 noiseOffset;
uniform vec2 noiseMotion;
uniform float noiseTiling;
uniform float noiseMagnitude = .1;
uniform float noiseEase = -2.0;
uniform float noiseSteps = 4.0;
uniform float noiseAdd = 0.0;
uniform sampler2D fireGradient;
uniform float energy = 1.0;

uniform float normalMultiply = 1.1;

varying vec2 v;

float ease(float x, float c) {
	if (x < 0f) x = 0f;
	if (x > 1f) x = 1f;
	
	if (c > 0f) {
		if (c < 1f) {
			return 1f - pow(1f - x, 1f / c);
		} else {
			return pow(x, c);
		}
	} else if (c < 0f) {
		if (x < 0.5f) {
			return pow(x * 2f, -c) * .5f;
		} else {
			return (1f - pow(1f - (x - .5f) * 2f, -c)) * .5f + .5f;
		}
	}
	
	return 0f;
}

void vertex() {
	v = VERTEX;
}

void fragment() {
	vec4 col = texture(TEXTURE, UV);
	
	vec2 n = textureLod(NORMAL_TEXTURE, UV, 0.0).rg * 2.0 - 1.0;
	n *= normalMultiply * col.a;
	float d = max(dot(-n, normalize(speed) * min(length(speed), maxSpeed)), 0.0);
	float t = clamp((d - maxSafeSpeed) / (maxSpeed - maxSafeSpeed), 0.0, 1.0);

	vec2 noiseUv = (v + noiseOffset + noiseMotion * TIME) / noiseTiling;
	float ns = texture(noise, noiseUv).r + noiseAdd;
	
	ns = ease(ns, noiseEase);
	ns = round(ns * noiseSteps) / noiseSteps;
	
	t -= ns * noiseMagnitude;
	t = clamp(t, 0, 1);
	
	vec4 fire = texture(fireGradient, vec2(t, 0.0));
	fire.rgb *= (1.0 + energy);
	col = mix(col, fire, fire.a);
	
	COLOR = col;
}"

