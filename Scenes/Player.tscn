[sub_resource type="Shader" id=23]
code = "shader_type canvas_item;

uniform float borderSize = 4.0;
uniform vec4 backgroundColor : hint_color = vec4(.75);
uniform vec4 progressColor : hint_color = vec4(1.0);
uniform sampler2D progressGradient;
uniform float progress : hint_range(0.0, 1.0) = .5;
uniform float dir : hint_range(-1.0, 1.0) = 1.0;
uniform float pulseFrequency = 2.0;
uniform float pulseMagnitude = 1.0;

varying vec2 v;

float Circle(vec2 center, float radius) {
	return 1.0 - clamp(length(center - v) / radius, 0.0, 1.0);
}

float CircleBorder(vec2 center, float radius, float border) {
	return clamp(1.0 - abs(border - Circle(center, radius) * radius) / border, 0.0, 1.0);
}

void vertex() {
	v = VERTEX;
}

void fragment() {
	vec2 size = v / UV;
	float border = borderSize;
	border += (sin(TIME * radians(360.0) * pulseFrequency) * .5 + .5) * pulseMagnitude;
	float c = CircleBorder(size / 2.0, min(size.x, size.y) / 2.0, border);
	
	c = step(0.001, c);
	
	vec2 off = v - size * .5;
	float t = atan(off.y, off.x) / radians(360.0);
	t = fract(t * dir);
	float prog = step(t, progress);
	
	COLOR = mix(backgroundColor, progressColor * texture(progressGradient, vec2(progress, 0.0)), prog);
	COLOR.a *= c;
}



"

