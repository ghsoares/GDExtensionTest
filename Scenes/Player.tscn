code = "shader_type canvas_item;

uniform mat4 globalTransform;

uniform float threshold = 0f;
uniform float borderTransition = 32f;
uniform sampler2D noise;
uniform vec2 noiseTiling = vec2(512f);
uniform vec2 noiseMotion = vec2(0f);
uniform float noiseInfluence = 0.1f;
uniform float noiseSteps = 2f;
uniform float noiseEase = -2f;
uniform sampler2D gradient;

varying vec2 v;

float Ease(float x, float c) {
	x = clamp(x, 0f, 1f);
	
	float curve1 = 1f - pow(1f - x, 1f / c);
	float curve2 = pow(x, c);
	
	float curve3 = pow(x * 2f, -c) * .5f;
	float curve4 = (1f - pow(1f - (x - .5f) * 2f, -c)) * .5f + .5f;
	
	float curveA = c < 1f ? curve1 : curve2;
	float curveB = x < .5f ? curve3 : curve4;
	
	return c == 0f ? 0f : (c > 0f ? curveA : curveB);
}

void vertex() {
	v = (globalTransform * vec4(VERTEX, 0f, 1f)).xy;
}

void fragment() {
	vec4 col = texture(TEXTURE, UV);
	float t = col.a;
	
	vec2 uv = (v + noiseMotion * TIME * 1f) / noiseTiling;
	
	float n = texture(noise, uv).r;
	t -= n * noiseInfluence;
	t = clamp(t, 0f, 1f);
	
	COLOR.a *= t > threshold ? 1f : 0f;
	
	t = floor(t * noiseSteps) / (noiseSteps - 1f);
	
	COLOR *= texture(gradient, vec2(t, 0f));
}



