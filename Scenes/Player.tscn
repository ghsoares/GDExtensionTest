[sub_resource type="Shader" id=6]
code = "shader_type canvas_item;

uniform float fresnelPower = 2.0;

void fragment() {
	vec3 normal;
	vec2 uv = (UV - .5) / .5;
	
	normal.xy = uv;
	normal.z = sqrt(1. - clamp(dot(uv, uv), 0., 1.));
	
	normal = normalize(normal);
	
	float d = sqrt(1.0 - dot(normal, vec3(0.0, 0.0, 1.0)));
	d = pow(d, fresnelPower);
	
	COLOR.a *= d;
	COLOR.a *= step(dot(uv, uv), 1.0);
}"

